    "from gtts import gTTS\n",
    "import os\n",
    "\n",
    "text = \"A red house. A big dog.\"\n",
    "tts = gTTS(text=text, lang=\"en\", slow=True)\n",
    "\n",
    "# Save the audio file\n",
    "tts.save(\"output.mp3\")\n",
    "\n",
    "# Play the saved audio file\n",
    "os.system(\"start output.mp3\")  # Windows"
   ]
